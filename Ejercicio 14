#include <iostream>
using namespace std;

void showMultiplicationTable(int number) {
    for (int i = 1; i <= 10; i++) {
        cout << number << " x " << i << " = " << number * i << endl;
    }
    cout << endl;
}

int main() {
    int num;
    cout << "Ingrese un nÃºmero entero: " ;
    cin >> num;
    
    showMultiplicationTable(num);
    return 0;
}
